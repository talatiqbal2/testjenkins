pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Building...'
      }
    }

    stage('Test') {
      steps {
        echo 'Testing...'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }

    stage('error') {
      steps {
        echo 'hello this is my message'
      }
    }

    stage('Fluffy Build') {
      steps {
        echo 'Fluffy Build'
      }
    }

    stage('Fluffy Test') {
      steps {
        echo 'Fluffy build'
      }
    }

    stage('Fluffy Deploy') {
      steps {
        echo 'Fluffy Deploy '
      }
    }

  }
}